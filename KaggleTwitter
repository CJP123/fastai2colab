      "cell_type": "code",
      "metadata": {
        "id": "o6fcJg7eDmnm",
        "colab_type": "code",
        "colab": {}
      },
      "source": [
        "def create_timm_body(arch, pretrained=True, cut=None):\n",
        "  model = arch\n",
        "  if cut is None:\n",
        "    ll = list(enumerate(model.children()))\n",
        "    cut = next(i for i,o in reversed(ll) if has_pool_type(o))\n",
        "  if isinstance(cut, int): return nn.Sequential(*list(model.children())[:cut])\n",
        "  elif callable(cut): return cut(model)\n",
        "  else: raise NamedError(\"cut must be either integer or function\")"
      ],
      "execution_count": 0,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "vpaSHjH0CwFy",
        "colab_type": "code",
        "colab": {}
