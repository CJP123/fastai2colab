    {
      "cell_type": "code",
      "metadata": {
        "id": "jKbop24iDDcu",
        "colab_type": "code",
        "colab": {}
      },
      "source": [
        "class MultiModel(Module):\n",
        "  \"A three-headed model given a `body` and `n` output features\"\n",
        "  def __init__(self, text:nn.Sequential,tab:nn.Sequential):\n",
        "    # nf = num_features_model(nn.Sequential(*body.children())) * (2)\n",
        "    self.text = text\n",
        "    self.tab = tab\n",
        "    self.head = create_head(2333, 2)\n",
        "  \n",
        "  def forward(self, x):\n",
        "    text = self.text(x[0])\n",
        "    tab = self.tab(x[1])\n",
        "    tot = text+tab\n",
        "    y = self.head(tot)\n",
        "    return y"
      ],
      "execution_count": 0,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "sLhYQzhpOxxs",
        "colab_type": "code",
        "colab": {}
      },
      "source": [
        "netx = MultiModel(learn_class.model[0], learn.model)"
      ],
      "execution_count": 0,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
